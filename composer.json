{
    "name": "avinashjoshi/cakephp-mapstore",
    "description": "MapStore - A Key Value store for CakePHP",
    "homepage": "https://github.com/avinashjoshi/cakephp-mapstore",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Avinash Joshi",
            "email": "a10i@icloud.com",
            "homepage": "http://a10i.me"
        }
    ],
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "cakephp/cakephp-codesniffer": "2.*",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "MapStore\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MapStore\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
